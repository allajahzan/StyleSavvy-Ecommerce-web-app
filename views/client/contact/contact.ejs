    <script>
        
  const scriptURL = 'https://script.google.com/macros/s/AKfycbxqJhMIy6ALoov3h7nesNWVDmDANSxA4xjfXrpTTUtEQHaqfEAyVFucBphHjfxP9gL8HQ/exec';
  const forms = document.getElementById('submit-to-google-sheet')

  forms.addEventListener('submit', e => {
    e.preventDefault()

    document.getElementById('submit_btn').setAttribute('disabled','disabled')
    document.getElementById('submit_btn').style.display = '0.8'
   
    fetch(scriptURL, { method: 'POST', body: new FormData(forms)})
      .then(response => 
        {   
             document.getElementById('submit_btn').removeAttribute('disabled')
             document.getElementById('submit_btn').style.display = '1'
            forms.reset();
            showSnackBar('Message has been sent successfully')
      
        })
      .catch(error => 
       
       { 
           
        window.location.href = '/500-Server-Error'
         
       }
      )
  });

  // show snack bar
function showSnackBar(text) {
    document.getElementById('snackbar_msg').innerHTML = text
    const x = document.getElementById("snackbar");
    x.className = "show";
    setTimeout(function () { x.className = x.className.replace("show", ""); }, 3000);
}




    </script>
      

